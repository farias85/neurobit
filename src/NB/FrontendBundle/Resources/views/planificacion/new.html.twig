{% extends 'BackendBundle:Layout:backend.html.twig' %}

{% block title %}{{ 'crud.create' | trans }} Planificación{% endblock %}
{% block nombre %}Planificación{% endblock %}
{% block subtitulo %}{{ 'crud.create' | trans }} Planificación{% endblock %}
{% block wrapper %}

    <div class="col-lg-6">
        <form action="{{ path('planificacion_new') }}" method="post" enctype="multipart/form-data">
            {{ form_rest(form) }}
            <br><input class="btn btn-success mt-10" type="submit" value="{{ 'crud.create' | trans }}"/>
        </form>
    </div>

    {% block wrapper_footer %} {% endblock %}

{% endblock %}

{% block footer %}
    <ul class="list-inline">
        <li>
            <a class="btn btn-primary" href="{{ path('planificacion_index') }}">{{ 'crud.list' | trans }}</a>
        </li>
    </ul>
{% endblock %}
